 <style name="TitleLabel" type="org.karora.cooee.app.Label">
  <properties>
   <property name="font">
    <font typeface="Verdana" size="18px"/>
   </property>
   <property name="foreground" value="#ffffff"/>
   <property name="layoutData">
    <layout-data type="org.karora.cooee.app.layout.SplitPaneLayoutData">
     <properties>
      <property name="alignment">
       <alignment horizontal="center"/>
      </property>
      <property name="background" value="#1b1b21"/>
      <property name="backgroundImage">
       <fill-image repeat="none">
        <image type="org.karora.cooee.app.ResourceImageReference">
         <resource-image-reference resource="/image/HeaderBackground.png"/>
        </image>
       </fill-image>
      </property>
      <property name="insets" value="8px"/>
      <property name="overflow" value="OVERFLOW_HIDDEN"/>
     </properties>
    </layout-data>
   </property>
  </properties>
 </style>
 
